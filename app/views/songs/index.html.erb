<h1>Songs Index:</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th>Genre</th>
      <th>Artist Name</th>
 
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
        <tr>
            <td><%= link_to song.title, song_path(song.id) %></td>
            <td> <%= song.release_year %></td>
            <td> <%= song.artist_name %></td>
            <td><%= song.genre %></td>
        </tr>
    <% end %>
</tbody>
</table>

<p>MENU:</p>
<div>
    <%= link_to "Create a New Song", new_song_path %>
</div>
